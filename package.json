  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Vizzuality/gfw-subscription-api/issues"
  },
  "homepage": "https://github.com/Vizzuality/gfw-subscription-api#readme",
  "engines": {
    "node": "~16.15"
  },
  "dependencies": {
    "axios": "^0.26.0",
